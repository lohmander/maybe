{
  "name": "@lohmander/maybe",
  "version": "0.0.0",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "type": "module",
  "files": [
    "build"
  ],
  "scripts": {
    "lint": "eslint \"src/**/*.{ts,tsx,js,jsx}\" --ignore-pattern \"**/*.test.*\" --ignore-pattern \"**/*.spec.*\" --ignore-pattern \"**/__tests__/**\" --fix",
    "typecheck": "tsc --noEmit",
    "test": "bun test",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,md}\"",
    "build": "tsc -p tsconfig.build.json && tsc -p tsconfig.build.json --emitDeclarationOnly --declaration --declarationMap",
    "prepare": "npm run build",
    "release": "semantic-release",
    "release:dry": "semantic-release --dry-run"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "prettier": "^3.6.2",
    "remeda": "^2.31.1",
    "typescript": "^5",
    "typescript-eslint": "^8.44.0",
    "semantic-release": "latest",
    "@semantic-release/changelog": "latest",
    "@semantic-release/commit-analyzer": "latest",
    "@semantic-release/git": "latest",
    "@semantic-release/github": "latest",
    "@semantic-release/npm": "latest",
    "@semantic-release/release-notes-generator": "latest",
    "@semantic-release/exec": "latest"
  },
  "peerDependencies": {
    "typescript": "^5"
  }
}
